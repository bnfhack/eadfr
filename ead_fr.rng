        <optional>
          <ref name="head"/>
        </optional>
        <zeroOrMore>
          <ref name="m.blocks"/>
        </zeroOrMore>
      </group>
      <choice>
        <group>
          <optional>
            <ref name="thead"/>
          </optional>
          <choice>
            <oneOrMore>
              <ref name="c"/>
              <optional>
                <ref name="thead"/>
              </optional>
            </oneOrMore>
            <oneOrMore>
              <ref name="c01"/>
              <optional>
