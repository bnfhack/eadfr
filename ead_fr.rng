                    <entry colname="anciennecote1">Ancienne cote</entry>
                    <entry colname="coteactuelle1">Cote actuelle</entry>
                    <entry colname="anciennecote2">Ancienne cote</entry>
                    <entry colname="coteactuelle2">Cote actuelle</entry>
                  </row>
                </thead>
