                <item>Centre des archives du monde du travail</item>
              </defitem>
              <defitem>
                <label>CAOM</label>
                <item>Centre des archives d'outre-mer</item>
              </defitem>
              <defitem>
                <label>CHAN</label>
                <item>Centre historique des Archives nationales</item>
              </defitem>
            </list>
          </a:example>
