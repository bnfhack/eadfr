      </choice>
      <ref name="m.inter"/>
    </choice>
  </define>
  <define name="am.dates.calendar">
    <optional>
      <attribute a:defaultValue="gregorian" name="calendar">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <define name="am.dates.era">
    <optional>
      <attribute a:defaultValue="ce" name="era">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <define name="am.dsctab.tpattern">
    <optional>
      <attribute name="tpattern">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <define name="yesorno">
    <data type="NMTOKEN"/>
  </define>
  <define name="a.level">
    <attribute name="level">
      <a:documentation>Niveau de description archivistique (liste close de valeurs normalisées). Si aucune valeur ne convient, voir @otherlevel</a:documentation>
      <choice>
        <value h:title="classe">class</value>
        <value h:title="collection">collection</value>
        <value h:title="dossier">file</value>
        <value h:title="fonds">fonds</value>
        <value h:title="pièce">item</value>
        <value h:title="autre niveau de description">otherlevel</value>
        <value h:title="groupe de documents">recordgrp</value>
        <value h:title="série organique">series</value>
        <value h:title="sous-fonds">subfonds</value>
        <value h:title="sous-groupe de documents">subgrp</value>
        <value h:title="sous-série organique">subseries</value>
      </choice>
    </attribute>
  </define>
  <define name="a.render">
    <optional>
      <attribute name="render">
        <a:documentation>Mise en forme typographique du contenu de l’élément</a:documentation>
        <choice>
          <value h:title="autre rendu">altrender</value>
          <value h:title="gras">bold</value>
          <value h:title="gras entre guillemets doubles">bolddoublequote</value>
          <value h:title="gras italique">bolditalic</value>
          <value h:title="gras entre guillemets simples">boldsinglequote</value>
          <value h:title="gras petites capitales">boldsmcaps</value>
          <value h:title="gras souligné">boldunderline</value>
          <value h:title="entre guillemets doubles">doublequote</value>
          <value h:title="italique">italic</value>
          <value h:title="châsse fixe">nonproport</value>
