      <optional>
        <attribute name="rowsep">
          <a:documentation>Séparation des des lignes</a:documentation>
          <ref name="yesorno"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="valign">
          <a:documentation>Alignement vertical</a:documentation>
          <choice>
            <value>top</value>
            <value>middle</value>
            <value>bottom</value>
          </choice>
